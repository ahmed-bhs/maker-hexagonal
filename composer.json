{
    "name": "adrienlbt/hexagonal-maker-bundle",
    "description": "Hexagonal Maker bundle add commands to Symfony Maker bundle to help you to create files for hexagonal architecture.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrien Lbt"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/http-kernel": "^6.4|^7.0",
        "symfony/framework-bundle": "^6.4|^7.0",
        "php": ">=8.1",
        "symfony/config": "^6.4|^7.0",
        "symfony/console": "^6.4|^7.0",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/yaml": "^6.4|^7.0",
        "doctrine/dbal": "^4.2",
        "symfony/process": "^6.4|^7.2",
        "symfony/maker-bundle": "^1.0@dev",
        "nikic/php-parser": "^5.4",
        "doctrine/orm": "^3.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "rregeer/phpunit-coverage-check": "dev-master",
        "squizlabs/php_codesniffer": "^3.7|^4.0",
        "symfony/phpunit-bridge": "^6.4.1|^7.0",
        "symfony/maker-bundle": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "AdrienLbt\\HexagonalMakerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "AdrienLbt\\HexagonalMakerBundle\\Tests\\": "tests/" }
    },
    "scripts": {},
    "extra": {
        "symfony": {
            "recipe": true
        }
    }
}
